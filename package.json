{
  "name": "nmo",
  "version": "1.1.0",
  "description": "A tool to manage your CouchDB cluster",
  "main": "lib/nmo.js",
  "bin": "bin/nmo-cli.js",
  "scripts": {
    "transpile": "babel src -d lib",
    "prepublish": "make && npm run transpile",
    "test": "npm run lint && lab -v --transform node_modules/lab-babel -S -I core,_babelPolyfill,regeneratorRuntime,__core-js_shared__,Reflect",
    "lint": "eslint ./src ./test"
  },
  "author": "Robert Kowalski <rok@kowalski.gd>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/robertkowalski/nmo/issues"
  },
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "src": "./src",
    "doc": "./doc",
    "man": "./man",
    "test": "./test",
    "website": "./website"
  },
  "dependencies": {
    "JSONStream": "^1.0.6",
    "bluebird": "~3.0.5",
    "bulkbadger": "^1.0.0",
    "config-chain": "~1.1.8",
    "couchbulkimporter": "^1.0.0",
    "csv-parse": "^1.0.0",
    "ini": "~1.3.3",
    "nopt": "~3.0.1",
    "npmlog": "~2.0.0",
    "osenv": "~0.1.0",
    "prettyjson": "^1.1.3",
    "valid-url": "~1.0.9",
    "wreck": "~7.0.0",
    "xtend": "~4.0.0"
  },
  "bundleDependencies": [
    "bluebird",
    "bulkbadger",
    "config-chain",
    "couchbulkimporter",
    "csv-parse",
    "ini",
    "JSONStream",
    "nopt",
    "npmlog",
    "osenv",
    "prettyjson",
    "valid-url",
    "wreck",
    "xtend"
  ],
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-eslint": "^4.1.5",
    "babel-polyfill": "^6.2.0",
    "eslint": "^1.9.0",
    "lab": "~8.0.0",
    "lab-babel": "1.0.0",
    "less": "~2.5.0",
    "marked-man": "~0.1.4",
    "nock": "^3.3.2",
    "node-doc-generator": "robertkowalski/node-documentation-generator#sloppy"
  }
}
