{
  "name": "nmo",
  "version": "1.0.1",
  "description": "A tool to manage your CouchDB cluster",
  "main": "lib/nmo.js",
  "bin": "bin/nmo-cli.js",
  "scripts": {
    "transpile": "babel src -d lib",
    "prepublish": "make && npm run transpile",
    "test": "lab -v --transform node_modules/lab-babel -t 100 -S"
  },
  "author": "Robert Kowalski <rok@kowalski.gd>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/robertkowalski/nmo/issues"
  },
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "src": "./src",
    "doc": "./doc",
    "man": "./man",
    "test": "./test",
    "website": "./website"
  },
  "dependencies": {
    "bluebird": "2.10.1",
    "config-chain": "~1.1.8",
    "ini": "~1.3.3",
    "nopt": "~3.0.1",
    "npmlog": "~1.2.0",
    "osenv": "~0.1.0",
    "valid-url": "~1.0.9",
    "wreck": "~5.6.0",
    "xtend": "~4.0.0"
  },
  "bundleDependencies": [
    "bluebird",
    "config-chain",
    "ini",
    "nopt",
    "npmlog",
    "osenv",
    "valid-url",
    "wreck",
    "xtend"
  ],
  "devDependencies": {
    "babel": "^5.1.10",
    "lab": "~5.5.1",
    "lab-babel": "1.0.0",
    "less": "~2.5.0",
    "marked-man": "~0.1.4",
    "node-doc-generator": "robertkowalski/node-documentation-generator#sloppy"
  }
}
